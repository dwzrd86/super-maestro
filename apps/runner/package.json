{
  "name": "@agentforge/runner",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "start": "bun run src/index.ts",
    "build": "bun build src/index.ts --outdir dist --target bun",
    "lint": "tsc --noEmit",
    "type-check": "bun x tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@agentforge/shared": "*"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.7.0"
  }
}
